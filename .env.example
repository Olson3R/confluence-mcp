# Confluence MCP Server Configuration
# Copy this file to .env and fill in your actual values

# Your Confluence domain (without trailing slash)
CONFLUENCE_BASE_URL=https://your-domain.atlassian.net

# Your Atlassian account email
CONFLUENCE_USERNAME=your-email@domain.com

# Your Atlassian API token (get from https://id.atlassian.com/manage-profile/security/api-tokens)
CONFLUENCE_API_TOKEN=your-api-token-here

# Comma-separated list of space keys you want to allow access to
ALLOWED_SPACES=SPACE1,SPACE2,SPACE3

# Enable debug logging (optional)
DEBUG=false